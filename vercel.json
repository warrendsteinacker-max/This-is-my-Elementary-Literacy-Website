{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/vite-project/dist/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/vite-project/dist/index.html"
    }
  ]
}